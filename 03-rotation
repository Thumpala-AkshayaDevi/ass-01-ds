def are_rotations(str1, str2):
    
    if len(str1) != len(str2):
        return False
    
    
    temp = str1 + str1

    if str2 in temp:
        return True

    return False
string1 = "rotation"
string2 = "tationro"
if are_rotations(string1, string2):
    print("The strings are rotations of each other.")
else:
    print("The strings are not rotations of each other.")
